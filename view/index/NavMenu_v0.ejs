<div class="banner kk_dd--f kk--f kk_ddf">
    <div class="box" id="body_parts_all">
        <el-menu class="b-nav " @open="handleOpen" @close="handleClose" :collapse="isCollapse" collapse-transition="false">
            <el-submenu index="item.id" v-bind:index="item.id" v-for="item in Body_parts_all" :key="item.id" show-timeout="1" hide-timeout="1" popper-append-to-body="true">
                <template slot="title">
                    <img v-bind:src="item.src" width="20"/>
                    <span slot="title">{{item.name}}</span>
                </template>
                <el-menu-item-group>
                    <el-menu-item v-bind:index="itemChildren.id" v-for="itemChildren in item.children" :key="itemChildren.id">
                        {{itemChildren.name}}</el-menu-item>
                </el-menu-item-group>
            </el-submenu>
        </el-menu>
    </div>
</div>